{
  "name": "@falcucci/changelog-it",
  "version": "1.5.5",
  "description": "Generates a changelog by matching git commits to Jira tickets.",
  "repository": "https://github.com/falcucci/changelog-it",
  "main": "dist/index.js",
  "bin": {
    "changelog-it": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "babel src --out-dir dist -s; cp changelog.config.js ./dist/",
    "clean": "rm -r dist || true",
    "test": "mocha",
    "prepare": "npm run build",
    "watch": "watch 'npm run build' ./src"
  },
  "author": "Jeremy Gillick",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "commander": "^2.11.0",
    "ejs": "^2.5.7",
    "form-urlencoded": "^2.0.4",
    "haikunator": "^2.0.0",
    "html-entities": "^1.2.1",
    "jira-client": "^6.4.0",
    "lodash": "^4.17.11",
    "node-fetch": "^1.7.3",
    "promise-throttle": "^0.3.1",
    "request": "^2.88.0",
    "simple-git": "^1.79.0",
    "source-map-support": "^0.5.0",
    "watch": "^1.0.2"
  }
}
